{
  "expo": {
    "name": "Taaruf Al Azhar",
    "slug": "taaruf-alazhar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "owner": "donarazhar",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
